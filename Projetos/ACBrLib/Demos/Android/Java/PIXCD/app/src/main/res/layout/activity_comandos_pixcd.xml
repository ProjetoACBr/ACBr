<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".comandos.ComandosPIXCDActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="238dp"
            android:layout_height="46dp"
            android:layout_marginTop="144dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="12dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.951">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="EndPoints"
                    android:textAlignment="center"
                    android:textSize="30dp"
                    android:textStyle="bold|italic"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="299dp"
            android:layout_height="203dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="202dp"
                android:orientation="vertical">

                <Button
                    android:id="@+id/btnQRCodeEstatico"
                    android:layout_width="295dp"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:text="QRCode EstÃ¡tico" />

                <Button
                    android:id="@+id/btnEndPointPix"
                    android:layout_width="295dp"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:text="/Pix" />

                <Button
                    android:id="@+id/btnEndPointCob"
                    android:layout_width="295dp"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:text="/Cob" />

                <Button
                    android:id="@+id/btnEndPointCobV"
                    android:layout_width="295dp"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:text="/CobV" />

            </LinearLayout>
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>