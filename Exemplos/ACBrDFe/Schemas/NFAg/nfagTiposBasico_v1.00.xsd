				</xs:unique>
				<xs:unique name="pk_nItemAG">
					<xs:selector xpath="./*"/>
					<xs:field xpath="@nItem"/>
				</xs:unique>
			</xs:element>
			<xs:element name="infNFAgSupl">
				<xs:annotation>
					<xs:documentation>Informações suplementares da NFAg</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="qrCodNFAg">
							<xs:annotation>
