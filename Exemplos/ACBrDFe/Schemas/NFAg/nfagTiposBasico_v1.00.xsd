										</xs:simpleType>
									</xs:element>
									<xs:element name="enderAgenciaAtend">
										<xs:annotation>
											<xs:documentation>Endereço da Agencia responsável pelo atendimento</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="TString">
												<xs:minLength value="2"/>
												<xs:maxLength value="200"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="gHistCons" maxOccurs="5">
										<xs:annotation>
											<xs:documentation>Histórico de Consumo</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="xHistorico">
													<xs:annotation>
														<xs:documentation>Nome do histórico para exibição/apresentação</xs:documentation>
														<xs:documentation>Exemplo: Consumo Meses Anteriores</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="TString">
															<xs:maxLength value="60"/>
															<xs:minLength value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="gCons" maxOccurs="13">
													<xs:annotation>
														<xs:documentation>Consumo</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="CompetFat" type="TCompet">
																<xs:annotation>
																	<xs:documentation>Ano e mês referência do faturamento (AAAAMM)</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="uMed" type="TUMedAG">
																<xs:annotation>
																	<xs:documentation>Unidade Básica de Medida</xs:documentation>
																	<xs:documentation>1 - m3 e 2 - litro</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="qtdDias">
																<xs:annotation>
																	<xs:documentation>Quantidade de dias de medição</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:whiteSpace value="preserve"/>
																		<xs:pattern value="[0-9]{5}"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="medDiaria" type="TDec_1302_1304" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Média Diária de consumo</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="consumo" type="TDec_1302" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Consumo no mês</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="volFat" type="TDec_1302">
																<xs:annotation>
																	<xs:documentation>Volume Faturado</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="medMensal" type="TDec_1302_1304">
