												<xs:maxLength value="10"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="dEmissSelo" type="TData" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Data de emissão do Selo de segurança do prestador</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="sRegulador" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Selo de segurança do Regulador</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="2"/>
												<xs:maxLength value="10"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="nAgenciaAtend">
