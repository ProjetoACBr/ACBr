														<xs:documentation>Média Mensal de consumo</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="gQualiAgua" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Informações sobre a qualidade de água</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CompetAnalise" type="TCompet">
										<xs:annotation>
											<xs:documentation>Ano e mês referência da analise (AAAAMM)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="gAnalise" maxOccurs="10">
										<xs:annotation>
											<xs:documentation>Grupo de informações da Análise</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="xItemAnalisado">
													<xs:annotation>
														<xs:documentation>Nome do item analisado</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="TString">
															<xs:maxLength value="60"/>
															<xs:minLength value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="nAmostraMinima" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Número de amostra minima</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="4"/>
															<xs:pattern value="[0-9]{4}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="nAmostraAnalisada" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Número de amostra analisada</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="4"/>
															<xs:pattern value="[0-9]{4}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="nAmostraFPadrao" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Número de amostra fora do padrão</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="4"/>
															<xs:pattern value="[0-9]{4}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="nAmostraDPadrao" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Número de amostra dentro do padrão</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="4"/>
															<xs:pattern value="[0-9]{4}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="nMediaMensal" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Media mensal dos valores da analise</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="TString">
															<xs:maxLength value="60"/>
															<xs:minLength value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="xValorReferencia" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Valor de referência</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="TString">
															<xs:maxLength value="60"/>
															<xs:minLength value="2"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Conclusao" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Conclusão da Análise</xs:documentation>
										</xs:annotation>
