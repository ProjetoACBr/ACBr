					<xs:attribute name="versao" type="TVerNFAg" use="required">
						<xs:annotation>
							<xs:documentation>Versão do leiaute</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Id" use="required">
						<xs:annotation>
							<xs:documentation>Identificador da tag a ser assinada</xs:documentation>
							<xs:documentation>Informar a chave de acesso da NFAg e precedida do literal "NFAg"</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:ID">
								<xs:pattern value="NFAG[0-9]{6}[A-Z0-9]{12}[0-9]{26}"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
				<xs:unique name="pk_nMedAG">
					<xs:selector xpath="./*"/>
