ence>									<xs:element name="nomeAgenciaAtend" minOccurs="0">										<xs:annotation>											<xs:documentation>Nome da Agencia responsável pelo atendimento</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="TString">												<xs:minLength value="2"/>												<xs:maxLength value="60"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="enderAgenciaAtend" minOccurs="0">										<xs:annotation>											<xs:documentation>Endereço da Agencia responsável pelo atendimento</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="TString">												<xs:minLength value="2"/>												<xs:maxLength value="200"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="sitioAgenciaAtend" minOccurs="0">										<xs:annotation>											<xs:documentation>Sítio eletrônico da Agencia</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="TString">												<xs:minLength value="2"/>												<xs:maxLength value="200"/>											</xs:restriction>										</xs:simpleType>									</xs:element>									<xs:element name="gHistCons" maxOccurs="5">										<xs:annotation>											<xs:documentation>Histórico de Consumo</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:sequence>												<xs:element name="xHistorico">													<xs:annotation>														<xs:documentation>Nome do histórico para exibição/apresentação</xs:documentation>														<xs:documentation>Exemplo: Consumo Meses Anteriores</xs:documentation>													</xs:annotation>													<xs:simpleType>														<xs:restriction base="TString">															<xs:maxLength value="60"/>															<xs:minLength value="2"/>														</xs:restriction>										